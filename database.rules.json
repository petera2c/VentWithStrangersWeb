{
  "rules": {
    "status": {
      ".read": true,
      "$uid": {
        ".write": "$uid === auth.uid"
      },
      ".indexOn": ["index", "state"]
    },
    "total_online_users2": {
      ".read": true,
      ".write": false
    },
    "is_typing": {
      "$conversationID": {
        "$uid": {
          ".read": true,
          ".write": "$uid === auth.uid"
        }
      }
    },
    "muted": {
      "$conversationID": {
        "$uid": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        }
      }
    },
    "block_check_new": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid",

        "$uid2": {
          ".read": "$uid === auth.uid || $uid2 === auth.uid",
          ".write": "$uid === auth.uid"
        }
      }
    }
  }
}
