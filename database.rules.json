{
  "rules": {
    "status": {
      ".read": true,
      "$uid": {
        ".write": "$uid === auth.uid"
      },
      ".indexOn": ["index", "state"]
    },
    "total_online_users2": {
      ".read": true,
      ".write": false
    },
    "$conversationID": {
      ".read": true,
      "$uid": {
        ".write": "$uid === auth.uid"
      }
    },
    "muted": {
      "$conversationID": {
        "$uid": {
          ".read": "$uid === auth.uid",
          ".write": "$uid === auth.uid"
        }
      }
    }
  }
}
